<?xml version="1.0"?>
<package format="2">
	<name>cob_keyframe_detector</name>
	<version>1.0.0</version>

	<description>
		Node that listens to TF, detects a robot movement and triggers a service.
	</description>

	<license>LGPL</license>

	<maintainer email="goa@ipa.fhg.de">Georg Arbeiter</maintainer>
	<author>Georg Arbeiter</author>

	<url>http://wiki.ros.org/cob_keyframe_detector</url>

	<buildtool_depend>catkin</buildtool_depend>

	<depend>cmake_modules</depend>
	<depend>dynamic_reconfigure</depend>
	<depend>roscpp</depend>
	<depend>sensor_msgs</depend>
	<depend>std_srvs</depend>
	<depend>tf</depend>
	<depend>tf_conversions</depend>
</package>
